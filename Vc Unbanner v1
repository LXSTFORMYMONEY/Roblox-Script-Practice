local ScreenGui: ScreenGui = Instance.new('ScreenGui', game.StarterGui);
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling;


local UICorner: UICorner = Instance.new('UICorner', ScreenGui);


local UIStroke: UIStroke = Instance.new('UIStroke', ScreenGui);


local LocalScript: LocalScript = Instance.new('LocalScript', ScreenGui);
LocalScript.Source = 'local textLabel = script.Parent\n\nlocal function getCurrentTime()\n	local t = os.date('*t')\n\n	local hour = t.hour\n	local minute = t.min\n	local ampm = 'AM'\n\n	if hour >= 12 then\n		ampm = 'PM'\n	end\n\n	hour = hour % 12\n	if hour == 0 then\n		hour = 12\n	end\n\n	return string.format('%02d:%02d %s', hour, minute, ampm)\nend\n\nwhile true do\n	textLabel.Text = getCurrentTime()\n	task.wait(1)\nend\n';


local Background_Frame: Frame = Instance.new('Frame', ScreenGui);
Background_Frame.Name = 'Background_Frame';
Background_Frame.BackgroundTransparency = 0.5;
Background_Frame.Position = UDim2.new(0.773279369, 0, 0.098, 0);
Background_Frame.BorderColor3 = Color3.fromRGB(0, 0, 0);
Background_Frame.Size = UDim2.new(0, 271, 0, 310);
Background_Frame.BorderSizePixel = 0;
Background_Frame.BackgroundColor3 = Color3.fromRGB(83, 83, 83);


local UICorner: UICorner = Instance.new('UICorner', Background_Frame);


