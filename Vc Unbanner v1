local close_menu_button: TextButton = Instance.new('TextButton', Background_Frame);
close_menu_button.FontFace = Font.new('rbxasset://fonts/families/PressStart2P.json', Enum.FontWeight.Bold, Enum.FontStyle.Italic);
close_menu_button.TextColor3 = Color3.fromRGB(255, 255, 255);
close_menu_button.BorderColor3 = Color3.fromRGB(0, 0, 0);
close_menu_button.Text = 'Close Menu';
close_menu_button.Name = 'close_menu_button';
close_menu_button.Position = UDim2.new(0.0877636, 0, 0.751, 0);
close_menu_button.Size = UDim2.new(0, 223, 0, 50);
close_menu_button.BorderSizePixel = 0;
close_menu_button.TextSize = 31;
close_menu_button.BackgroundColor3 = Color3.fromRGB(0, 0, 0);


local LocalScript: LocalScript = Instance.new('LocalScript', close_menu_button);
LocalScript.Source = 'local button = script.Parent\n\nbutton.MouseButton1Click:Connect(function()\n	local screenGui = button:FindFirstAncestorOfClass('ScreenGui')\n	if screenGui then\n		screenGui.Enabled = false\n	end\nend)\n';


local UICorner: UICorner = Instance.new('UICorner', close_menu_button);


local invisible_button: TextButton = Instance.new('TextButton', Background_Frame);
invisible_button.FontFace = Font.new('rbxasset://fonts/families/PressStart2P.json', Enum.FontWeight.Bold, Enum.FontStyle.Italic);
invisible_button.TextColor3 = Color3.fromRGB(255, 255, 255);
invisible_button.BorderColor3 = Color3.fromRGB(0, 0, 0);
invisible_button.Text = 'Invisible';
invisible_button.Name = 'invisible_button';
invisible_button.Position = UDim2.new(0.084073566, 0, 0.356, 0);
invisible_button.Size = UDim2.new(0, 223, 0, 50);
invisible_button.BorderSizePixel = 0;
invisible_button.TextSize = 31;
invisible_button.BackgroundColor3 = Color3.fromRGB(0, 0, 0);


local LocalScript: LocalScript = Instance.new('LocalScript', invisible_button);
LocalScript.Source = 'local Players = game:GetService('Players')\nlocal player = Players.LocalPlayer\nlocal button = script.Parent\n\nlocal isInvisible = false\nlocal highlight = nil\nlocal UNDERGROUND_OFFSET = -2 -- studs under baseplate\n\n-- Make character invisible/visible using LocalTransparencyModifier\nlocal function setInvisible(character, invisible)\n	local transparencyValue = invisible and 1 or 0\n\n	for _, part in pairs(character:GetDescendants()) do\n		if part:IsA('BasePart') or part:IsA('MeshPart') then\n			part.LocalTransparencyModifier = transparencyValue\n			part.CanCollide = not invisible\n		elseif part:IsA('Accessory') and part:FindFirstChild('Handle') then\n			part.Handle.LocalTransparencyModifier = transparencyValue\n			part.Handle.CanCollide = not invisible\n		end\n	end\nend\n\nbutton.MouseButton1Click:Connect(function()\n	local character = player.Character\n	if not character then return end\n	local root = character:FindFirstChild('HumanoidRootPart')\n	if not root then return end\n\n	if not isInvisible then\n		-- Move underground once\n		root.CFrame = root.CFrame * CFrame.new(0, UNDERGROUND_OFFSET, 0)\n\n		-- Make invisible\n		setInvisible(character, true)\n\n		-- Create outline\n		highlight = Instance.new('Highlight')\n		highlight.Adornee = character\n		highlight.FillTransparency = 1\n		highlight.OutlineTransparency = 0\n		highlight.OutlineColor = Color3.fromRGB(0, 255, 255)\n		highlight.Parent = player:WaitForChild('PlayerGui')\n\n		isInvisible = true\n	else\n		-- Restore visibility\n		setInvisible(character, false)\n\n		-- Remove outline\n		if highlight then\n			highlight:Destroy()\n			highlight = nil\n		end\n\n		isInvisible = false\n	end\nend)\n';


local UICorner: UICorner = Instance.new('UICorner', invisible_button);


local speed_button: TextButton = Instance.new('TextButton', Background_Frame);
speed_button.FontFace = Font.new('rbxasset://fonts/families/PressStart2P.json', Enum.FontWeight.Bold, Enum.FontStyle.Italic);
speed_button.TextColor3 = Color3.fromRGB(255, 255, 255);
speed_button.BorderColor3 = Color3.fromRGB(0, 0, 0);
speed_button.Text = 'Speed';
speed_button.Name = 'speed_button';
speed_button.Position = UDim2.new(0.0877636, 0, 0.549, 0);
speed_button.Size = UDim2.new(0, 223, 0, 50);
speed_button.BorderSizePixel = 0;
speed_button.TextSize = 31;
speed_button.BackgroundColor3 = Color3.fromRGB(0, 0, 0);


local Speed_script: LocalScript = Instance.new('LocalScript', speed_button);
Speed_script.Source = 'local Players = game:GetService('Players')\nlocal player = Players.LocalPlayer\nlocal button = script.Parent  -- make sure this is the TextButton\n\nlocal isFast = false  -- tracks toggle state\n\nbutton.MouseButton1Click:Connect(function()\n	local character = player.Character\n	if not character then return end\n	local humanoid = character:FindFirstChildOfClass('Humanoid')\n	if not humanoid then return end\n\n	if not isFast then\n		humanoid.WalkSpeed = 50\n		isFast = true\n	else\n		humanoid.WalkSpeed = 13  -- default speed\n		isFast = false\n	end\nend)\n';
Speed_script.Name = 'Speed_script';


local UICorner: UICorner = Instance.new('UICorner', speed_button);


